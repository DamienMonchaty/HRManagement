@model IEnumerable<HRManagement.Web.Models.Client>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(m => m.Name)</th>
            <th>@Html.DisplayNameFor(m => m.Description)</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var c in Model)
        {
        <tr>
            <td>@c.Name</td>
            <td>@c.Description</td>
            <td>
                <a class="btn btn-outline-info" asp-controller="Client" asp-action="Edit" asp-route-id="@c.Id">Editer</a>
                <a class="btn btn-outline-primary" asp-controller="Client" asp-action="Delete" asp-route-id="@c.Id">Supprimer</a>
            </td>
        </tr>
        }
    </tbody>
</table>
